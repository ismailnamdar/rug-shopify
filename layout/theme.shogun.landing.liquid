<script>

    $(document).ready( function () {
      jquery2("#LeadFormTY").on("hidden.bs.modal", function () {
     	 activatePromoCode();
		});

      $("#LeadFormsDemo input").focus(function(){
        $("#LeadFormsDemo .invalid-feedback").hide(300);
      });

      $('.subscribe-form-lead').submit( function (e) {
        e.preventDefault();

        enrollNewUser($(this));
      });

    });

    function activatePromoCode(){
      var url = new URL(window.location.href);
      var query_string = url.search;
      var search_params = new URLSearchParams(query_string);
      search_params.append('cc', 'welcome');
      search_params.append('f', '8');
      url.search = search_params.toString();
      window.location.href = url.toString();
    }

    function enrollNewUser(form){
      $.ajax({
        type: form.attr('method'),
        url:  'https://a.klaviyo.com/ajax/subscriptions/subscribe',
        data: 'g=' + window.newsletterListID + '&email=' + form.find('[name="EMAIL"]').val(),
        error       : function(err) { alert('We ran into an error while trying to add you to our newsletter. Please try again later.'); },
        success     : function(res) {
          var closestModal = $(form).closest('.modal.fade');
          jquery2(closestModal).bsModal("hide");
          Cookies.set('alreadyEnrolled', "true");
          ga('send', 'event', 'LeadForm', 'SubmitLeadForm'); // Send this action back to GA and GO.
					{% if document.getElementById('persistent-badge') %}
          	document.getElementById('persistent-badge').style.visibility = 'hidden';
					{% endif %}
          reportLeadConversion();
         	jquery2("#LeadFormTY").bsModal("show");
        }
      });
    }

    function reportLeadConversion(){
      fbq('track', 'Lead', {
      content_name: 'popup',
  	  });	//Track the conversion back to Facebook pixel.
    }

    function openLeadForm(leadFormId){
      ga('send', 'event', 'LeadForm', 'openedFromBadge', leadFormId);
      jquery2(leadFormId).bsModal('show');
    }


    function closeLeadForm(leadFormId){
      jquery2(leadFormId).bsModal('hide');
    }

    function openOFormFromSaveToday(){
      ga('send', 'event', 'LeadForm', 'openedFromBadge', '#LeadFormO');
      openOriginalLeadForm();
	}

  </script>
