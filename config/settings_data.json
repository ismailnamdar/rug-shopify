          "a24b2ff9-0d7f-4f0d-a616-063914c9b254": {
            "type": "cook",
            "settings": {
              "hidden": false,
              "title": "Cook",
              "text": "<p>This free kit includes three full-of-love recipes to make and share!</p>",
              "url_1": "",
              "image_1": "shopify://shop_images/FIL_LP_Crouton_Salad.png",
              "image_size_1": 305,
              "title_1": "Cupid’s Crouton Salad",
              "text_1": "<p>Fall in love with <em>panzanella</em>, an Italian salad made with garlic bread, mozzarella, and chopped garden vegetables.</p>",
              "url_2": "",
              "image_2": "shopify://shop_images/FIL_LP_Lasagna_Soup.png",
              "image_size_2": 305,
              "title_2": "Hearty Lasagna Soup",
              "text_2": "<p>Snuggle up with a cozy bowl of soup featuring beefy broth, squiggly noodles, and melted cheese!</p>",
              "url_3": "",
              "image_3": "shopify://shop_images/FIL_LP_love_Muffins.png",
              "image_size_3": 305,
              "title_3": "Raspberry Love Muffins",
              "text_3": "<p>Bake for someone you love! Make fluffy muffins loaded with raspberries and chocolate chips.</p>"
            }
          },
