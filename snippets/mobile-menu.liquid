<div class="mobile-menu">
  <div class="page-width mobile-menu-content">
    <div class="flex flex-center space mobile-menu__header">
      <h3>
        <svg xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" width="56" height="56" viewBox="0 0 56 56">
          <image id="Logo_Icon_small220x220px" width="56" height="56" xlink:href="data:image/png;base64,"/>
        </svg>
      </h3>

      {% comment %}{{ 'close-pink.svg' | asset_url | img_tag: 'Close', 'close-nav' }}{% endcomment %}

      <span class="close-nav">x</span>
    </div>

    <div class="form-search-mobile">
      <form action="/search" method="get" role="search" id="mobile-search-box">
        <input type="hidden" name="type" value="article,product,page">
        <div class="flex align-center">
          <label for="Search" class="label-hidden">
            {{ 'general.search.placeholder' | t }}
          </label>
          <input type="search"
                 name="q"
                 id="Search"
                 value="{{ search.terms | escape }}"
                 placeholder="{{ 'general.search.placeholder' | t }}">
          <button type="submit" class="btn">
            <i class="fa fa-search"></i>
            <span class="icon-fallback-text">{{ 'general.search.submit' | t }}</span>
          </button>
        </div>
      </form>    
    </div><!-- /.form-search-mobile -->

    <div class="mobile-menu__body">
      <ul class="nav-menu">
        {% for link in menus[section.settings.main_linklist].links %}
          {% assign childlink_handle = link.title | handle %}
          {% if menus[childlink_handle].links != blank %}
            <li>
              <a href="{{ link.url }}" class="js-trigger-mobile-nav">
                {{ link.title }} 

                <svg class="trigger-secondary" xmlns="http://www.w3.org/2000/svg" width="11.218" height="19.769" viewBox="0 0 11.218 19.769">
                  <path id="Path_2" data-name="Path 2" d="M0,0,8.533,8,16,0" transform="translate(1.885 17.884) rotate(-90)" fill="none" stroke="#4d4d4d" stroke-linecap="round" stroke-linejoin="round" stroke-miterlimit="10" stroke-width="2.667"/>
                </svg>
              </a>

              {% assign childlink_handle = link.handle %}

              <ul class="secondary-links">
                <li class="secondary-links__title">
                  <h5 class="js-menu-back">
                      <svg xmlns="http://www.w3.org/2000/svg" width="11.218" height="19.769" viewBox="0 0 11.218 19.769">
                        <path id="Path_2" data-name="Path 2" d="M0,0-8.533,8-16,0" transform="translate(9.333 17.884) rotate(90)" fill="none" stroke="#919191" stroke-linecap="round" stroke-linejoin="round" stroke-miterlimit="10" stroke-width="2.667"/>
                      </svg>

                      {{ link.title }}
                  </h5>
                </li><!-- /.secondary-links__title -->

                {% for childlink in linklists[childlink_handle].links %}
                  <li data-link-handle="{{childlink.title | handle}}">{{ childlink.title | link_to: childlink.url }}</li>
                {% endfor %}
              </ul>
            </li>
          {% else %}
            <li><a href="{{ link.url }}">{{ link.title }}</a></li>
          {% endif %}
        {% endfor %}

        {% if customer %}
        <li class="mobile-account__nav">
          <a href="#" class="js-trigger-mobile-nav">
            Hello, {{ customer.first_name }}!
            <svg class="trigger-secondary" xmlns="http://www.w3.org/2000/svg" width="11.218" height="19.769" viewBox="0 0 11.218 19.769">
              <path id="Path_2" data-name="Path 2" d="M0,0,8.533,8,16,0" transform="translate(1.885 17.884) rotate(-90)" fill="none" stroke="#4d4d4d" stroke-linecap="round" stroke-linejoin="round" stroke-miterlimit="10" stroke-width="2.667"></path>
            </svg>
            <ul class="secondary-links account-secondary">
              <li class="secondary-links__title">
                <h5 class="js-menu-back">
                    <svg xmlns="http://www.w3.org/2000/svg" width="11.218" height="19.769" viewBox="0 0 11.218 19.769">
                      <path id="Path_2" data-name="Path 2" d="M0,0-8.533,8-16,0" transform="translate(9.333 17.884) rotate(90)" fill="none" stroke="#919191" stroke-linecap="round" stroke-linejoin="round" stroke-miterlimit="10" stroke-width="2.667"></path>
                    </svg>

                    Hello, {{ customer.first_name }}!
                </h5>
              </li><!-- /.secondary-links__title -->

              
              {% if customer.metafields.subscriptions.customer_string %}<a data-recharge-token href="/tools/recurring/portal/{{customer.metafields.subscriptions.customer_string}}/payment_sources"  name="billing" class="nav-item-alt">Account</a>{% endif %}
              {% if customer.metafields.subscriptions.customer_string %}<a data-recharge-token href="/tools/recurring/portal/{{customer.metafields.subscriptions.customer_string}}/subscriptions" name="memberships" class="nav-item-alt"><span>Memberships</span></a>{% endif %}
              <a href="/account/logout">Log Out</a>
              
            </ul>

            
          </a>
        </li>
        {% endif %}
        
        {% comment %}
        <li>
          <a href="/search"><i class="fa fa-search"></i> Search</a>
        </li>
        {% endcomment %}
      </ul>

      <div class="other-buttons">
        <a href="/pages/join" class="btn full">Join Now</a>
        {% if customer %}
        {% comment %}
          <a data-recharge-token href="/tools/recurring/portal/{{customer.metafields.subscriptions.customer_string}}/subscriptions#overview" class="btn alt full">My Account</a>
        {% endcomment %}
        {% else %}
        <a href="/account/login" class="btn alt full">My Account</a>
        {% endif %}
      </div>

      <div class="social-icons-mobile">
        <ul class="social-icons">
          <li>
            <a href="{{- settings.social_facebook_link -}}" class="icon-facebook" title="Raddish Kids on Facebook"></a>
          </li>

          <li>
            <a href="{{- settings.social_instagram_link -}}" class="icon-instagram" title="Raddish Kids on Instagram"></a>
          </li>

          <li>
            <a href="{{- settings.social_pinterest_link -}}" class="icon-pinterest" title="Raddish Kids on Pinterest"></a>
          </li>

          <li>
            <a href="{{- settings.social_twitter_link -}}" class="icon-twitter" title="Raddish Kids on Twitter"></a>
          </li>
        </ul>      
      </div><!-- /.social-icons-mobile -->
    </div><!-- /.mobile-menu__body -->
  </div>
</div>
