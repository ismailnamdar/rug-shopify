{% assign _section = section.settings %}
{% assign _blocks = section.blocks %}

{% render 'images' _blocks: _blocks%}

{% schema %}
{
  "name": "Images secondary",
  "tag": "section",
  "class": "section-images slick-images",
  "settings": [],
  "max_blocks": 3,
  "blocks": [
  {
    "type": "image",
    "name": "image",
    "settings": [
    {
      "type": "image_picker",
      "id": "image",
      "label": "Image"
    }]
  }]
}
{% endschema %}
