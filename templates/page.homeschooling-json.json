    "customer_reviews_997Q7U": {
      "type": "customer-reviews",
      "blocks": {
        "review_XipxVx": {
          "type": "review",
          "settings": {
            "review": "“My kids read through everything in the kit and make a grocery list. Then they work together to make dinner for the family! It's a wonderful way to learn teamwork, math communication, and cooking!”",
            "author": "- Michelle, Rad Mom",
            "stars": "5",
            "width": "260",
            "mobile_width": 48
          }
        },
        "review_8Pa6AG": {
          "type": "review",
          "settings": {
            "review": "“We do one Raddish recipe a week! They are really engaging and cover more than just cooking. Raddish kits cover math history, geography and more. We love how it enriches our homeschooling!”",
            "author": "- Danielle, Rad Mom",
            "stars": "5",
            "width": "260",
            "mobile_width": 48
          }
        },
        "review_VNnr3q": {
          "type": "review",
          "settings": {
            "review": "My son is autistic and a extremely picky eater. It has helped him to try new textures and flavors. He's also learned many life skills like grocery shopping and paying a cashier, measuring, estimating, etc.",
            "author": "- Sally-Ann, Rad Mom",
            "stars": "5",
            "width": "260",
            "mobile_width": 48
          }
        },
        "review_63FAbi": {
          "type": "review",
          "settings": {
            "review": "My grandkids have learned Irish Dance, can find Greece on the map, know how birds build nests, and are well versed in many culinary techniques. We are building lifelong memories together!",
            "author": "- Donna, Rad Grandma",
            "stars": "5",
            "width": "260",
            "mobile_width": 48
          }
        },
        "review_JUYVQk": {
          "type": "review",
          "settings": {
            "review": "My girls are 8 & 6 and new to cooking. We spent several hours on our first box. We consider it a science class as well as language and math with the reading and measuring.",
            "author": "- Leah, Rad Mom",
            "stars": "5",
            "width": "260",
            "mobile_width": 48
          }
        }
      },
