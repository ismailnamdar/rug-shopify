/*================ MODULES ================*/
/*================ Site Header ================*/
#shopify-section-header {
  position: relative;
  z-index: 2;
}

.site-header {
  z-index: 2;
}

.promo-bar {
  @include prefix(transition, all 300ms, webkit spec);
  max-height: 9999px;
  overflow: hidden;

  strong {
    font-weight: 600;
  }

  .page-width {
    padding-top: 10px;
    padding-bottom: 10px;
    padding-right: 55px;
    padding-left: 55px;
    position: relative;
    font-size: 1.3rem;
    font-weight: 500;
  }
  &.collapsed {
    max-height: 0;
  }
  p {
    margin: 0;
  }
  .promo-content a {
    display: inline-block;
    text-decoration: underline;

    &:hover {
      text-decoration: none;
    }
  }
  .close-bar {
    position: absolute;
    top: calc(50% + 2px);
    right: 20px;
    line-height: 1;
    @include prefix(transform, translateY(-50%), webkit spec);
    img {
      width: 16px;
      height: 16px;
    }
  }
  @media (min-width: 450px){
    .page-width {
      font-size: 1.6rem;
    }
    .close-bar {
      right: 30px;
      img {
        width: 21px;
        height: 21px;
      }
    }
  }

  @media (max-width: 767px) {
    .page-width {
      font-size: 1.4rem;
    }
  }
}

.logo-nav {
  @media (max-width: 875px){
    justify-content: center;
    width: 100%;
    h1, div.logo {
      width: 100%;
    }
  }
}

.site-header .logo-nav .logo {
  @media (max-width: 876px){
    overflow: visible;
    width: auto;
    z-index: 20000;
  }
}

h1.logo, div.logo {
  margin: 0;
  padding: 8px 0;
  text-align: center;

  @media (min-width: 875px) {
    padding: 16px 0;
  }
}

.site-logo {
  overflow: visible;
  width: 100%;
  display: flex !important;
  img {
    display: inline-block;
    width: 50%;
    max-width: 105px;
    padding: 13px 0 16px 0;

    @media (max-width: 876px){
      position: relative;
      width: 100px !important;
      padding: 8px 0 !important;
    }
    @media(max-width: 700px){
      right: -59.5%;
    }
    @media(max-width: 650px){
      right: -63.5%;
    }
    @media(max-width: 600px){
      right: -66.5%;
    }
    @media(max-width: 376px){
      right: -69.5%;
    }
    @media (min-width: 876px){
      width: 88px;
    }
    @media (min-width: 910px){
      width: 88px !important;
    }
    @media (max-width: 400px){
      width: 105px !important;
    }
  }
}

.site-header {
  position: relative;
}

.mobile-menu-trigger {
  width: 30px !important;
  height: 60px !important;
  position: relative;
  position: absolute;
  top: calc(50% + 2px);
  left: 17px;
  @include prefix(transform, translateY(-50%), webkit spec);

  span:first-child {
    position: absolute;
    overflow: hidden;
    clip: rect(0 0 0 0);
    height: 1px;
    width: 1px;
    margin: -1px;
    padding: 0;
    border: 0;
    border-radius: 3px;
  }
  span:last-child {
    top: 46%;
    position: absolute;
    left: 50%;
    transform: translate(-50%, -50%);
    display: block;
    width: 24px;
    height: 3px;
    background-color: #4D4D4D;
    @include prefix(transition, background-color 300ms, webkit spec);
    &:before, &:after {
      content: '';
      display: block;
      width: 24px;
      height: 3px;
      background-color: #4D4D4D;
      @include prefix(transition, background-color 300ms, webkit spec);
    }
    &:before {
      @include prefix(transform, translateY(-8px), webkit spec);
    }
    &:after {
      @include prefix(transform, translateY(5px), webkit spec);
    }
  }
  @media (min-width: 450px){
    width: 30px;
    height: 30px;
    top: calc(50% + 3px);
    span:last-child {
      width: 30px;
      height: 4px;
      &:before, &:after {
        width: 30px;
        height: 4px;
      }
      &:before {
        @include prefix(transform, translateY(-10px), webkit spec);
      }
      &:after {
        @include prefix(transform, translateY(6px), webkit spec);
      }
    }
  }
  @media (min-width: 876px){
    display: none;
  }
}

.cart-account {
  position: absolute;
  top: calc(50% + 8px);

  @media (max-width: 875px) {
    padding-bottom: 10px;
  }

  @media (min-width: 876px){
    top: calc(50% + 4px);
  }

  right: 20px;
  @include prefix(transform, translateY(-50%), webkit spec);
  .join-link, .cart-link {
    line-height: 1;
  }
  .join-link {
    @media (min-width: 876px){
      display: none;
    }
  }
  .join-link img {
    width: 24px;
  }
  .cart-link {
    position: relative;
    margin-right: 8px;
    img {
      width: 24px;
    }
    span {
      position: absolute;
      top: -15px;
      left: 54%;
      @include prefix(transform, translateX(-50%), webkit spec);
      color: $pink;
      font-size: 16px;
      font-weight: bold;
    }
  }
  .join-button, .login-button {
    display: none;
    @media (min-width: 876px){
      display: inline-block;
    }
  }
  .login-button {
    padding: 12px 28px 8px 8px;
    font-family: $secondary-bold;
    border: 0;
    font-size: 1.4rem;
    line-height: 1.285;
    color: #4D4D4D;
    text-transform: capitalize;
    &:hover {
      border: 0;
    }
  }
  .join-button {
    font-weight: 900;
    font-size: 1.6rem;
    letter-spacing: 0.03125em;
    padding: 14px 32px 14px;
    border-radius: 4px;
    margin-top: -22px !important;
  }
  @media (min-width: 450px){
    right: 30px;
    .join-link img {
      width: 30px;
    }
    .cart-link img {
      width: 28px;
    }
    .cart-link {
      margin-right: 12px;
    }
  }
}

#shopify-section-header {
  min-height: 0 !important;
}

.nav-wrapper {
  justify-content: space-between;
  width: 78%;
}

ul.site-nav {
  display: none;
  @media (min-width: 876px){
    display: block;
    list-style: none;
    margin: 0 auto 0 30px !important;
    padding: 8px 0 0 !important;
    li {
      display: inline-block;
      position: relative;
      &:not(:last-of-type){
        margin-right: 15px;
      }
      &.site-nav--has-submenu {
        line-height: 1;
        img {
          width: 14px;
        }
      }
      a {
        color: #4D4D4D;
        font-family: $block;
        text-decoration: none;
        font-size: 1.4rem;
        text-transform: capitalize;
        &:hover {
          color: $pink;
        }
        img {
          width: 14px;
          margin-left: 4px;
        }
        .fa-search {
          font-size: 1.6rem;
        }
      }
      ul {
        display: none;
        list-style: none;
        margin: 0;
        padding: 0;
        position: absolute;
        top: 100%;
        left: -24px;
        width: 200px;
        li {
          display: block;
          padding-left: 0;
          &:before {
            display: none;
          }
          margin: initial;
          &:not(:last-of-type) {
            border-bottom: 2px solid #c7edf4;
            margin-right: 0;
          }
          &:first-of-type a {
            padding-top: 20px;
          }
          a {
            padding: 14px 20px 14px 24px;
            background-color: #fff;
            display: block;
            color: $blue;
            font-size: 1.1rem;
            &:hover {
              color: $pink;
            }
          }
        }
      }
      &:hover ul {
        display: block;
      }
    }
    &.nav-action {
      margin: 0 81px 0 19px !important;
      cursor: pointer;
      .login-button {
        margin-right: 36px;
