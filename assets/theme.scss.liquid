.cart__note {
  text-align: left;
  width: 300px;
  input[type="checkbox"] {
    display: none;
  }
  .checkbox {
    position: relative;
    label:before {
      content: "";
      display: inline-block;
      border: 2px solid $light-blue;
      width: 18px;
      height: 18px;
      margin-right: 10px;
      position: absolute;
      left: 0;
      bottom: 1px;
    }
    label {
      display: inline-block;
      cursor: pointer;
      position: relative;
      padding-left: 25px;
      margin-right: 15px;
      color: #333;
      text-transform: initial;
    }
  }
  input[type=checkbox]:checked + label:before {
    content: "\f00c";
    font-family: "fontawesome";
    font-size: 1.5rem;
    color: $color-body-text;
    text-align: center;
    line-height: 14px;
    width: 18px;
    height: 18px;
  }
  .gift-note {
    label {
      display: block;
      color: #333;
      padding-bottom: 4px;
    }
    textarea {
      // border: 2px solid $light-blue;
      width: 300px;
      height: 70px;
      padding: 8px;
    }
  }
  @media (max-width: 750px){
    width: 100%;
    .gift-note textarea {
      width: 100%;
    }
  }
}

