// Browse by Category
.browse-by-category {
	border-top: 1px solid #fdb7c0;
	border-bottom: 1px solid #fdb7c0;
	position: relative;
	// padding: 10px 0;
	text-align: center;
	margin-bottom: 30px;
	@include prefix(transition, border-bottom 10ms, webkit spec);
	ul {
		list-style: none;
		margin: 4px 0 0;
		padding: 0;
		overflow: hidden;
		background-color: #fff;
		display: none;
		@include prefix(transition, max-height 300ms, webkit spec);
	}
	&.open {
		border-bottom: 1px solid #fff;
		ul {
			li:last-of-type a {
				// border-bottom: 1px solid $pink;
			}
		}
		.cat-dropdown {
			@include prefix(transform, rotate(180deg), webkit spec);
		}
	}
	h2 {
		font-family: $block;
		color: $pink;
		text-transform: capitalize;
		letter-spacing: 0.1em;
		font-size: 1.0rem;
		border-bottom: 1px solid $pink;
		margin: 16px auto;
		text-align: center;
		display: inline-block;
	}
	.cat-dropdown {
		width: 10px;
		@include prefix(transition, transform 300ms, webkit spec);
	}
	li {
		margin: 0;
		a {
			color: $pink;
			text-transform: capitalize;
			font-family: $block;
			font-size: 1.3rem;
			padding: 12px 30px 10px;
			border-bottom: 1px solid #fdb7c0;
			display: block;
			&:hover {
				color: $green;
			}
		}
		&:last-of-type a {
			border-bottom: 0;
		}
		&:first-of-type a {
			padding-top: 4px;
		}
	}
	@media (max-width: 875px){
		.page-width {
			padding: 0;
		}
		ul {
			position: absolute;
			left: 0;
			top: calc(100% - 4px);
			width: 100%;
			z-index: 999;
		}
	}
	@media (min-width: 876px){
		text-align: left;
		border-top: 1px solid #eaeaea;
		border-bottom: 1px solid #eaeaea;
		h2 {
			display: none;
			border-bottom: 0;
			margin-right: 16px;
			font-size: 1.2rem;
			img {
				display: none;
			}
			@media (min-width: 1095px){
				display: inline;
			}
		}
		ul {
			display: inline !important;
		}
		li {
			display: inline-block;
			a {
				font-family: $primary;
				color: $color-primary;
				text-transform: initial;
				border: 0;
				padding: 12px 8px 10px;
				font-size: 1.4rem;
			}
			&:first-of-type a {
				padding-left: 0;
			}
			&:last-of-type a {
				padding-right: 0;
			}
		}
	}
}


