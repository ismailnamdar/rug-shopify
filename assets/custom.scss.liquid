    width: 123px;
    min-width: 123px;
    height: 123px;
    display: flex;
    justify-content: center;
    align-items: center;
    overflow: hidden;
    margin: 0 auto;
    transition: width .3s, height .3s, min-width .3s, left .3s, top .3s;
    z-index: 30;
    position: absolute;
    left: calc(50% - 61px);
    top: calc(50% - 61px);
    cursor: pointer;

    @media #{$mobile} {
      margin: 0;
      width: 82px;
      min-width: 82px;
      height: 82px;
      left: calc(50% - 41px);
      top: calc(50% - 41px);
    }
  }

  .stat__image img {
    width: 110px;
    transition: width .3s;

    @media #{$mobile} {
      width: 67px;
    }
  }

  .stat__title {
    font-size: 40px;
    line-height: 1.25;
    color: #01BCBE;
    margin: 0 auto -2px;
    font-family: $KGHappy;
    max-width: 243px;

    @media #{$mobile} {
      font-size: 32px;
    }
  }

  .stat__entry {
    font-size: 18px;
    line-height: 1.44;
    margin: auto;
    color: #4D4D4D;
    font-weight: 900;
    max-width: 310px;
    width: 100%;

    @media #{$mobile} {
      font-size: 18px;
    }
  }

  .stat__entry p {
    margin: 0;
  }
}

/* ------------------------------------------------------------ *\
  Stat Is Active
\* ------------------------------------------------------------ */

.stat.is-active {

  .stat__image {
    width: 160px;
    min-width: 160px;
    height: 160px;
    z-index: 7;
    left: calc(50% - 80px);
    top: calc(50% - 80px);

    @media #{$mobile} {
      width: 107px;
      min-width: 107px;
      height: 107px;
      left: calc(50% - 53px);
      top: calc(50% - 53px);
    }

    @media (max-width: 350px) {
      width: 94px;
      min-width: 94px;
      height: 94px;
      left: calc(50% - 47px);
      top: calc(50% - 47px);
    }
  }

  .stat__image img {

    @media #{$mobile} {
      width: 97px;
    }

    @media (max-width: 350px) {
      width: 79px;
    }
  }

  .stat__content {
    pointer-events: auto;
    opacity: 1;
  }
}

/* ------------------------------------------------------------ *\
  Section Brands
\* ------------------------------------------------------------ */

.section-brands {
  padding: 10px 0 40px;
  background-color: #E4F4F6;
  background-size: 200px;
  overflow: hidden;

  @media #{$mobile} {
    padding: 13px 0 24px;
  }

  .section__title {
    font-family: $KGHappy;
    font-size: 24px;
    line-height: 1.58;
    text-align: center;
    color: #4D4D4D;
    margin-bottom: 33px;

    @media #{$mobile} {
      font-size: 20px;
      line-height: 1.3;
      margin: 11px 0 16px;
    }
  }

  .section__content {
    height: 111px;
    position: relative;
  }
}

/* ------------------------------------------------------------ *\
  Brands
\* ------------------------------------------------------------ */

.brands {
  display: flex;
  left: 50%;
  position: absolute;
  top: 0;
  transform: translateX(-50%);
  min-width: 100vw;
  padding: 0 20px;

  .brands__item {
    flex: 0 0 153px;
    max-width: 153px;
  }

  .brands__item + .brands__item {
    margin-left: 40px;
  }
}

.brands--copy {
  left: 50%;
  transform: translateX(calc(-150% - 40px));
}

.brands--copy2 {
  transform: translateX(calc(50% + 40px));
}

@keyframes mymove {
  0%   {transform: translateX(-50%);}
  100% {transform: translateX(-150%);}
}

@keyframes mymoveCopy {
  0%   {transform: translateX(-150%);}
  100% {transform: translateX(-250%);}
}

@keyframes mymoveCopy2 {
  0%   {transform: translateX(50%);}
  100% {transform: translateX(-50%);}
}

@keyframes wiggle {
  0%   {transform: translateY(0)}
  25%   {transform: translateY(15px)}
  50%   {transform: translateY(0)}
  75%   {transform: translateY(15px)}
  100%   {transform: translateY(0)}
}

.wiggle-animate {
  animation: wiggle 1s !important;
}

/* ------------------------------------------------------------ *\
  Brand
\* ------------------------------------------------------------ */

.brand {
  border-radius: 8px;
  overflow: hidden;
  background-color: #fff;
  width: 153px;
  height: 111px;
  display: flex;
  justify-content: center;
  align-items: center;
  box-shadow: 0px 2px 4px 0px rgba(77,77,77,0.2);

  img {
    width: 140px;
  }
}

/* ------------------------------------------------------------ *\
  Section Text Image
\* ------------------------------------------------------------ */

.section-text-image {
  padding: 51px 0 19px;

  @media #{$mobile} {
    padding: 25px 0;
  }

  strong {
    font-size: 18px;
    font-family: $KGHappy;
    font-weight: normal;
    color: #01BCBE;
    margin: 0 0 10px;

    @media #{$mobile} {
      margin-bottom: 11px;
      line-height: 1.61;
      letter-spacing: 0;
    }
  }

  p {
    font-size: 18px;
    line-height: 1.45;
    margin: 0;

    @media #{$mobile} {
      line-height: 1.45;
    }
  }

  p + p {
    margin-top: 25px;

    @media #{$mobile} {
      margin-top: 27px;
    }
  }

  p strong {
    font-family: $KGHappy;
    color: #01BCBE;
    line-height: 1.45;
    letter-spacing: -0.07em;

    @media #{$mobile} {
      line-height: 1.62;
    }
  }

  a {
    transition: color .3s;

    &:hover {
      color: #01BCBE;
    }
  }

  .section__shell {
    max-width: 1217px;
    padding: 0 30px;
    margin: auto;

    @media #{$mobile} {
      padding: 0 15px;
    }
  }

  .section__title {
    font-size: 40px;
    font-family: $KGHappy;
    color: #01BCBE;
    margin: 0 0 18px;

    @media #{$mobile} {
      font-size: 32px;
      line-height: 1.56;
      margin-bottom: 8px;
    }
  }

  .section__entry {
    @media #{$mobile} {
      padding-right: 8px;
    }
  }

  .section__item {
    display: flex;
    margin: 0 -28px;

    @media #{$tablet-portrait} {
      margin: 0 -15px;
    }

    @media #{$mobile} {
      display: block;
      margin: 0;
    }
  }

  .section__content,
  .section__media {
    padding: 0 28px;
    flex: 0 0 50%;
    max-width: calc(50% - 20px);

    @media #{$tablet-portrait} {
      padding: 0 15px;
      max-width: 50%;
    }

    @media #{$mobile} {
      max-width: 100%;
      padding: 0;
    }
  }

  .section__media {
    padding-left: 40px;

    @media #{$tablet-portrait} {
      padding-left: 15px;
    }

    @media #{$mobile} {
      margin-top: 20px;
      padding: 0;
    }
  }

  .section__image {
    padding: 13px 0;
    position: relative;
    margin-bottom: 14px;

    @media #{$mobile} {
      padding: 3px 13px 3px 0;
      margin-left: -6px;
      margin-bottom: 22px;
    }
  }

  .section__image img {
    border-radius: 16px;
  }

  .section__image span {
    display: inline-block;
    position: relative;
    z-index: 10;
    max-width: 100%;
  }

  .section__image-shadow {
    position: absolute;
    left: 4px;
    top: 26px;
    height: calc(100% - 29px);
    width: calc(100% + 12px);
    border-radius: 16px;
    z-index: 9;

    @media #{$mobile} {
      left: 18px;
      top: 23px;
      height: calc(100% - 12px);
      width: calc(100% - 11px);
    }
  }

  .section__dots {
    border-top: 6px dotted #a9dd61;
    height: 6px;
    width: 100%;
    margin: 71px 0 31px;

    @media #{$tablet-portrait} {
      margin: 28px 0;
    }
  }

  .section__media .section__entry {
    padding-left: 10px;

    @media #{$tablet-portrait} {
      padding-left: 0;
    }

    @media #{$mobile} {
      margin: 0 -9px 0 -2px;
      padding-left: 5px;
    }
  }

  .section__media .section__entry p {

    @media #{$mobile} {
      line-height: 1.45;
    }
  }

  .section__item--reversed .section__content {
    order: 2;
    padding: 16px 28px 0 35px;

    @media #{$tablet-portrait} {
      padding: 0 15px;
    }

    @media #{$mobile} {
      padding: 0;
    }
  }

  .section__item--reversed .section__title {
    margin-bottom: 10px;
  }

  .section__item--reversed .section__media {
    order: 1;
    padding-right: 21px;

    @media #{$mobile} {
      margin-top: 22px;
      padding-right: 0;
    }

  }
  .section__item--reversed .section__image {

    @media #{$mobile} {
      padding-right: 0;
      margin-left: 0;
      padding-left: 9px;
    }
  }

  .section__item--reversed .section__image-shadow {
    left: -14px;
    top: 30px;
    height: calc(100% - 34px);
    width: calc(100% + 6px);

    @media #{$mobile} {
      left: -3px;
      top: 14px;
      height: calc(100% - 11px);
      width: calc(100% - 5px);
    }
  }
}

/* ------------------------------------------------------------ *\
  Section-raya-50-50
\* ------------------------------------------------------------ */

.section-raya-50-50 {
