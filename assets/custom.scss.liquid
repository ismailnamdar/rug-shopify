    align-self: flex-start;
    width: 100%;
    margin-bottom: 5px;
    font-family: "KG HAPPY Solid", sans-serif;
    font-size: 32px;
    line-height: 1.34375;

    sup {
      font-size: 20px;
      line-height: 1.35;
    }

    span {
      display: inline-block;
      vertical-align: middle;
      margin-left: 10px;
      font: 400 16px/1.625 "Avenir Book", sans-serif;
    }
  }

  .plan__entry {
    width: 100%;
    margin-bottom: 24px;
    font: 400 16px/1.625 "Avenir Book", sans-serif;

    p {
      margin: 0 0 5px;

      &:last-child {
        margin-bottom: 0;
      }
    }

    strong {
      font-weight: 900;
      font-family: "Avenir", sans-serif;
    }
  }

