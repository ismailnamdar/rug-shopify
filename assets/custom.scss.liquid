    margin: 0;
  }
}

/* Mac and iOS specific */

.ios .plan {
  .plan__title {
    padding: 6px 5px 4px;
  }
}

.plan {
  flex: 0 0 25%;
  max-width: 324px;
  display: flex;
  margin-bottom: 40px;
  padding: 0 12px;

  &.js-tab {
    @media #{$mobile} {
      display: none;
      max-width: 345px;

      &.is-active {
        display: block;
      }
    }
  }

  .plan__inner {
    width: 100%;
    display: flex;
    flex-flow: row wrap;
    position: relative;
    padding: 32px 16px 24px;
    border: 1px solid rgba(#4d4d4d, 0.12);
    text-align: center;
    border-radius: 8px;
    box-shadow: 0 2px 4px 0 rgba(#4d4d4d, 0.2);

    @media (max-width: 1200px) {
      padding: 32px 10px 24px;
    }
  }

  .plan__title {
    position: absolute;
    top: 0;
    left: 50%;
    transform: translate(-50%, -50%);
    min-width: 165px;
    max-width: 100%;
    background: $blue;
    margin: 0;
    padding: 3px 5px 7px;
    font-family: "KG HAPPY Solid", sans-serif;
    font-size: 12px;
    line-height: 1.333333;
    color: #fff;
    text-transform: uppercase;
    border-radius: 8px;
  }

