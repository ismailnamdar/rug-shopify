/* ------------------------------------------------------------ *\
  Section Promo Banner
\* ------------------------------------------------------------ */

.section-promo-banner {
  font-size: 18px;
  line-height: 1.44;
  padding: 25px 0;

  .section__cols {
    max-width: 1180px;
    display: flex;
    flex-flow: row wrap;
    margin: 0 auto;
  }

  .section__col {
    flex: 1;
    min-width: 50%;
    position: relative;
  }

  .section__col:first-child {
    padding-left: 48px;
  }

  .section__col:first-child:last-child {
    padding-left: 0;
  }

  .section__col + .section__col {
    padding-left: 42px;

    &:before {
      content: '';
      background-image: url("{{- 'green-line-vertical.png' | asset_url- }}");
      background-repeat: no-repeat;
      background-size: cover;
      background-position: center center;
      width: 7px;
      height: 100%;
      position: absolute;
      top: 0;
      left: -5px;
    }
  }

  .section__inner {
    display: flex;
    flex-flow: row wrap;
    align-items: center;
    padding-top: 16px;
  }

  .section__content {
    flex: 1;
    padding-left: 24px;
    padding-top: 5px;
    padding-right: 10px;
  }

  .section__image {
    width: 75px;
    margin-bottom: 0;
  }

  h2 {
    font-family: "KG HAPPY Solid", sans-serif;
    font-size: 24px;
    line-height: 1.33;
    margin-bottom: 4px;
    margin-top: 0;
  }

  p a {
    font-weight: 900;

    &:hover {
      text-decoration: underline;
    }
  }

  p {
    margin: 0;
  }

  /* Tablet Portrait */

  @media #{$tablet-portrait} {
    padding-left: 20px;

    .section__col:first-child {
      padding-left: 0;
    }

    .section__col + .section__col {
      padding-left: 30px;
    }

    .section__content {
      padding-right: 15px;
    }
  }

  /* Mobile */

  @media #{$mobile} {
    text-align: center;
    max-width: calc(100% - 30px);
    padding: 24px 11px 25px;
    border-radius: 4px;
    margin: 0 auto;
    box-shadow: 0 2px 4px rgba(77, 77, 77, 0.2);

    .section__cols {
      display: block;
    }

    .section__col {
      min-width: auto;
    }

    .section__inner {
      display: block;
      padding-top: 0;
    }

    .section__image {
      width: 60px;
    }

    .section__content {
      padding: 0;
    }

    h2 {
      font-size: 20px;
      line-height: 1.3;
      margin-bottom: 2px;
    }

    .section__col + .section__col {
      padding-left: 0;

      &:before {
        background-image: url("{{- 'green-line.png' | asset_url- }}");
        display: block;
        width: 100%;
        height: 6px;
        margin-top: 24px;
        margin-bottom: 19px;
        position: static;
      }
    }
  }
}



/* ------------------------------------------------------------ *\
  Navigation updates
\* ------------------------------------------------------------ */

@media (min-width: 876px) {
  .site-header ul.site-nav li ul.active-dropdown {
    display: block;
  }

  .site-header ul.site-nav li a:hover {
    color: #f3576b;

    .ico-nav-angle {
      path {
        stroke: #919191 !important;
      }
    }
  }
  .site-header ul.site-nav li.site-nav--has-submenu > a:hover {
    color: #919191;
  }

  .site-header ul.site-nav li:hover > a
  {
    .ico-nav-angle {
      transform: rotate(0deg);  -webkit-transform: rotate(0deg);  -moz-transform: rotate(0deg);
    }
  }

  .site-header ul.site-nav li a.active-dropdown {
    color: #919191;
  }
}

.site-header ul.site-nav li a .ico-nav-angle {
  width: 22px;
  height:  12px;
  transform: rotate(180deg);  -webkit-transform: rotate(180deg);  -moz-transform: rotate(180deg);
}



/* Mobile menu */


.mobile-menu ul.nav-menu:after,
.mobile-menu ul.nav-menu li:after,
.mobile-menu ul.nav-menu:first-of-type:before,
.mobile-menu ul.nav-menu ul.secondary-links li:first-of-type:before {
  display:none;
}

.mobile-menu {
  left: 0;
  visibility: hidden;
  opacity: 0;
  transition: all 0.3s;
  padding-right: 60px;
  background-color: rgba(255, 255, 255, 0.7);

  ul.nav-menu {
    overflow: visible;
    flex: 1;
    position: static;

    &:before {
      display: none !important;
    }
  }

  .page-width {
    padding-bottom: 0;
  }

  ul.nav-menu li ul.secondary-links {
    position: absolute;
    top: 10px;
    left: 0;
    background-color: #fff;
    bottom: 0;
    height: auto !important;
    overflow: visible;
    transform: translateX(-110%);
    transition: all 0.3s;
    overflow: auto;

    li a {
      color: #4D4D4D;
      font-size:16px;
    }

    .secondary-links__title {

      h5 {
        position: relative;
        text-transform: uppercase;
        color: #919191;
        font-size: 16px;
        margin: 0;
        padding: 16px 0 6px;
        font-family: "KG HAPPY Solid",sans-serif;
      }

      svg {
        position: absolute;
        top: 16px;
        right: 100%;
        margin-right: 8px;
      }
    }

    &.secondary-links-active {
      transform: translateX(0);
    }
  }

  ul.nav-menu li a svg {
    display: inline-block;
    vertical-align: middle;
    margin: -5px 10px 0 0;

    @media (max-width: 767px) {
      float: right;
      margin: 3px 28px 0 0;
    }
  }

  ul.nav-menu li a {
    color: #4D4D4D;
    padding: 14px 0 6px;
    letter-spacing: 0;
  }

  ul.nav-menu > li > a {
    padding: 14px 0 12px 16px;
    letter-spacing: 0.07em;
  }

  .close-nav {
    font-size:24px;
    color: #f3576b;
    font-family: "KG HAPPY Solid",sans-serif;
    position: relative;
    top: 4px;
  }

  .mobile-menu__body {
    flex:1;
    /*display: flex; */
    /*flex-flow: column wrap;*/
    position: relative;
    overflow: auto;
  }

  .mobile-menu__header.flex:first-of-type {
    padding: 0 24px 13px 4px;

    svg {
      margin: 0 0 0 -15px;
      position: relative;
      top: 3px;
    }
  }

  .mobile-menu-content {
    transform: translateX(-100%);
    padding-right: 10px;
    padding-left:15px;
    transition: all 0.3s;
    height: 100%;
    display: flex;
    flex-flow: column wrap;
    border-right:1px solid #eaeaea;
    box-shadow: 2px 0px 7px 0px rgba(77,77,77,0.15);
    position: relative;
    overflow: hidden;
  }

  &.open {
    visibility: visible;
    opacity: 1;

    .mobile-menu-content {
      transform: translateX(0);
    }
  }

  .form-search-mobile {
    padding-bottom: 6px;
    padding-right: 5px;
  }

  form .flex:first-of-type {
    padding: 0;
    box-shadow:         0px 2px 5px 0px rgba(0, 0, 0, 0.34);
    border-radius: 5px;
    overflow: hidden;
  }

  button[type="submit"] {
    padding: 11px 0 18px 0;
    width:56px;
  }

  input[type="search"] {
    border:0;
    width:calc(100% - 56px);
    padding: 0 4px 2px 11px;
    font-family: "KG HAPPY Solid",sans-serif;
    letter-spacing: 0.05em;

    &::placeholder {
      text-transform: uppercase;
      color: #919191;
    }
  }

  .other-buttons {
    margin-top: 14px;
    padding-right: 5px;

    .btn {
      padding: 16px 0;
    }

    .btn.alt {
      padding: 14px 0;
      margin-top:17px;
    }
  }

  .social-icons {
    list-style:none; list-style-position: outside;
    margin: 0;
    padding: 0;
    display: flex;
    flex-flow: row wrap;
    align-items: center;
    justify-content: center;
    padding-top: 15px;
    margin-bottom: 40px;

    li {
      padding: 0 12px;
    }

    a {
      display: block;
      width: 40px;
      height: 40px;
      background-size: contain;
      background-position: center center;
      background-repeat: no-repeat;
