/* ------------------------------------------------------------ *\
  List Media
\* ------------------------------------------------------------ */

.list-media {
  position: relative;
  display: flex;
  flex-flow: row wrap;
  justify-content: center;
  align-items: center;
  margin: 0 -60px;
  padding: 80px 0;
  list-style-type: none;

  &:before,
  &:after {
    content: "";
    position: absolute;
    top: 0;
    left: 50%;
    width: calc(100% + 110px);
    height: 40px;
    transform: translateX(-50%);
    border-bottom:6px dotted #36a5a8;
  }

  &:after {
    top: auto;
    bottom: 0;
    border-top:6px dotted #36a5a8;
    border-bottom:0;
  }

  li {
    flex: 0 0 155px;
    max-width: 155px;
    padding: 0 30px;
  }

  @media #{$tablet-portrait} {
    li {
      flex:  0 0 120px;
      max-width: 120px;
      padding: 0 20px;
    }
  }

  @media #{$mobile} {
    li {
      flex: 0 0 25%;
      max-width: 25%;

      &:first-child {
        display: none;
      }

      &:last-child {
        display: none;
      }
    }
  }

  @media #{$mobile-small} {
    padding: 62px 0;
    margin: 0 -10px;

    &:before,
    &:after {
      background-size: 300% auto;
    }
  }
}

