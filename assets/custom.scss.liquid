// Site colors
$pink: #f3576b;
$green: #a9dd61;
$blue: #01bcbe;
$blue-alt: #8bf0f1;
$light-blue: #edfafc;

/* ------------------------------------------------------------ *\
  Fonts
\* ------------------------------------------------------------ */

@font-face {
  font-family: "Avenir";
  src: url({{-"Avenir-Medium.woff2"|asset_url-}}) format("woff2"),
    url({{-"Avenir-Medium.woff"|asset_url-}}) format("woff");
  font-weight: 500;
  font-style: normal;
}

@font-face {
  font-family: "Avenir";
  src: url({{-"Avenir-Roman.woff2"|asset_url-}}) format("woff2"),
    url({{-"Avenir-Roman.woff"|asset_url-}}) format("woff");
  font-weight: normal;
  font-style: normal;
}

@font-face {
  font-family: "Avenir";
  src: url({{-"Avenir-Heavy.woff2"|asset_url-}}) format("woff2"),
    url({{-"Avenir-Heavy.woff"|asset_url-}}) format("woff");
  font-weight: 900;
  font-style: normal;
}

@font-face {
  font-family: "Avenir Book";
  src: url({{-"Avenir-Book.woff2"|asset_url-}}) format("woff2"),
    url({{-"Avenir-Book.woff"|asset_url-}}) format("woff");
  font-weight: normal;
  font-style: normal;
}

@font-face {
  font-family: 'KG HAPPY Solid';
  src: url({{-"KGHAPPYSolid.woff2"|asset_url-}}) format('woff2'),
    url({{-"KGHAPPYSolid.woff"|asset_url-}}) format('woff');
  font-weight: normal;
  font-style: normal;
  font-display: swap;
}

$KGHappy: 'KG HAPPY Solid', sans-serif;

/*  Responsive Helpers  */

$responsive-xxs: 480px;
$responsive-xs: 767px;
$responsive-sm: 1023px;
$responsive-md: 1200px;
$responsive-lg: 1400px;

$mobile-small: "(max-width: #{$responsive-xxs}) ";
$mobile: "(max-width: #{$responsive-xs}) ";
$tablet-portrait: "(max-width: #{$responsive-sm}) ";
$tablet-portrait-min: "(min-width: #{$responsive-xs} + 1) ";
$small-desktop: "(max-width: #{$responsive-md}) ";
$desktop: "(max-width: #{$responsive-lg}) ";
$retina: "(min-resolution: 2dppx) ";

.visible-xs-block,
.visible-xs-inline,
.visible-xs-inline-block,
.visible-sm-block,
.visible-sm-inline,
.visible-sm-inline-block,
.visible-md-block,
.visible-md-inline,
.visible-md-inline-block,
.visible-lg-block,
.visible-lg-inline,
.visible-lg-inline-block {
  display: none !important;
}

@media (max-width: $responsive-xs) {
  .hidden-xs {
    display: none !important;
  }
  .visible-xs-block {
    display: block !important;
  }
  .visible-xs-inline {
    display: inline !important;
  }
  .visible-xs-inline-block {
    display: inline-block !important;
  }
}

@media (min-width: $responsive-xs + 1) and (max-width: $responsive-sm) {
  .hidden-sm {
    display: none !important;
  }
  .visible-sm-block {
    display: block !important;
  }
  .visible-sm-inline {
    display: inline !important;
  }
  .visible-sm-inline-block {
    display: inline-block !important;
  }
}

@media (min-width: $responsive-sm + 1) and (max-width: $responsive-md) {
  .hidden-md {
    display: none !important;
  }
  .visible-md-block {
    display: block !important;
  }
  .visible-md-inline {
    display: inline !important;
  }
  .visible-md-inline-block {
    display: inline-block !important;
  }
}

@media (min-width: $responsive-md + 1) {
  .hidden-lg {
    display: none !important;
  }
  .visible-lg-block {
    display: block !important;
  }
  .visible-lg-inline {
    display: inline !important;
  }
  .visible-lg-inline-block {
    display: inline-block !important;
  }
}

/* ------------------------------------------------------------ *\
  Header
\* ------------------------------------------------------------ */

.promo-bar {
  &.promo-bar--hide {
    /*max-height: 0;*/
  }

  .page-width {
    @media (max-width: 875px) {
      padding-top: 8px;
      padding-bottom: 8px;
    }
  }
}

.site-header {
  border-bottom: 1px solid #eaeaea;
  box-shadow: 0 2px 4px rgba(77, 77, 77, 0.15);
  .page-width {
    max-width: 1290px;
    padding: 0;
  }
  ul.site-nav {
    padding-top: 16px;
    margin: 0 0 0 65px;
    li {
      a {
        display: block;
        font-size: 14px;
        font-family: "KG HAPPY Solid";
        letter-spacing: 0;

        img {
          width: 22px;
        }
      }

      .search-toggle:hover svg * {
        fill: #f3576b !important;
      }

      .cart-link,
      .search-toggle,
      .login-button--nav {
        display: inline-block;
        padding: 0;
      }

      .cart-link {
        position: relative;
        padding: 4px 0 8px;
        bottom: -4px;

        &:hover img {
          opacity: 0.7;
        }

        img {
          width: 29px;
        }

        span {
          position: absolute;
          top: -17px;
          right: 2px;
          width: 20px;
          font-size: 16px;
          color: #F3576B;
          font-family: 'Avenir';
          font-weight: 900;
          text-align: center;

          @media (max-width: 1180px){
            width: 15px;
            top: -10px;
            font-size: 14px;
          }

          @media(max-width: 767px) {
            top: -17px;
            left: 60%;
          }
        }
      }

      .login-button--nav {
        position: relative;
        margin: -2px 33px 0 29px;
        vertical-align: text-top;

        &:before {
          content: '';
          position: absolute;
          left: -16px;
          bottom: calc(50% + 1px);
          width: 2px;
          height: 22px;
          background-color: #01BCBE;
          border-radius: 3px;
          transform: translateY(50%);
        }

        @media (max-width: 1250px) {
          margin: 0 20px 0 23px;

          &:before {
            left: -14px
          }
        }
      }
    }

    > li {
      > a {
        padding: 10px 0;
      }
    }

    li:not(:last-of-type) {
      margin-right: 38px;
    }

    li:last-of-type {
      margin-left: 5px;
      vertical-align: middle;
    }

    .site-nav__submenu {
      padding-bottom: 10px;
      background-color: #fff;
      box-shadow: 0 2px 4px rgba(77, 77, 77, 0.15);
      border-radius: 0 0 4px 4px;
      border:1px solid #efefef;

      li {
        border-bottom: 0;
        margin-right: 0;
      }

      li:last-of-type {
        margin-left: 0;
      }
    }

    @media (max-width: 1250px) {
      margin: 0 0 0 40px;
    }
  }

  .logo {
    padding: 0;
    overflow: hidden;

    @media (max-width: 875px) {
      padding: 6px 0;
    }
  }

  .site-logo {
    display: block;
    max-height: 100px;
    img {
      width: 207px;
    }
  }

  .cart-account {
    right: 17px;
    top: calc(50% + 12px);

    @media (max-width: 875px) {
      padding-bottom: 2px;
    }

    .login-button {
      padding: 4px 28px 11px 8px;
      font-size: 14px;
      font-family: "KG HAPPY Solid";
    }

    .search-toggle {
      display: none;
      color: #4D4D4D;
      font-size: 20px;

      @media (max-width: 875px) {
        display: inline-block;
      }
    }

    .cart-link {
      display: none;
      color: #4d4d4d !important;

      @media (max-width: 875px) {
        display: inline-block;
        margin-left: 16px;

        &:before {
          content: '';
          position: absolute;
          left: -8px;
          bottom: 55%;
          width: 2px;
          height: 22px;
          background-color: #01BCBE;
          border-radius: 3px;
          transform: translateY(50%);
        }
      }
    }

    .join-button {
      font-family: "Avenir Black", font-sans;
      font-weight: 700;
      margin-left: 11px;
      margin-top: -7px;
      padding: 15px 39px;
    }
  }

  @media (max-width: 1180px) {
    .page-width {
      padding: 0;
    }

    ul.site-nav {
      > li:not(:last-of-type) {
        margin-right: 20px;
      }
    }

    .cart-account {
      right: 15px;
      .join-button {
        padding: 17px 28px;
        margin-left: 0;
      }

      .login-button {
        font-size: 12px;
      }
    }
  }

  @media (max-width: 1080px) {
    ul.site-nav {
      > li:not(:last-of-type) {
        margin-right: 10px;
      }

      li {
          a {
            font-size: 12px;
          }
        }
    }
  }

  @media (max-width: 980px) {
    .site-logo {
      img {
        width: 170px;
      }
    }

    ul.site-nav {
      > li:not(:last-of-type) {
        margin-right: 10px;
      }
    }

    .cart-account {
      .cart-link {
        margin-right: 0;
      }

      .login-button {
        padding: 13px 8px;
        margin-left: 5px;
      }
      .join-button {
        padding: 13px 8px;
      }
    }
  }

  @media (max-width: 800px) {
    .cart-account {
      .cart-link {
        margin-right: 5px;
      }
    }
  }

  @media #{$mobile} {
    .logo-nav {
      .logo {
        font-size: 0;
      }
    }
    .site-logo {
      display: inline-block;
      height: 70px;
      font-size: 0;
    }
  }
}

#shopify-section-header {
  &.sticky {
    z-index: 11;

    .header__inner {
      position: fixed;
      top: 0;
      left: 0;
      width: 100%;
      background-color: #fff;
    }

    @media (max-width: 875px) {
      .logo {
        padding: 0;
        height: 63px;
      }

      .site-logo {
        img {
          width: auto;
          height: 65px;
        }
      }
    }
  }
}

@media (min-width: 876px) {
  ul.site-nav {
    li {
      ul.site-nav__submenu {
        width: auto;
        white-space: nowrap;
        padding-top: 18px;
        li {
          a {
            padding: 10px 20px 10px 24px;
            color: #4d4d4d;

            &:hover {
              color: $pink;
            }
          }
        }
      }
    }
  }
}

/* ------------------------------------------------------------ *\
  Footer
\* ------------------------------------------------------------ */

.pre-footer {
  position: relative;
  max-width: 100%;
  overflow: hidden;

  .page-width {
    max-width: 1355px;
    padding: 0 30px;
  }

  .blocks {
    display: flex;
  }

  .block {
    position: relative;
    display: flex;
    flex-wrap: wrap;
    min-height: 176px;
    padding: 53px 22px 24px;
    flex: 1 1 100%;
    color: #fff;

    > * {
      flex: 0 0 100%;
    }

    a {
      color: #fff;
    }

    h3 {
      font-family: "KG HAPPY Solid";
      margin: 0 0 16px;
      font-size: 24px;
      font-weight: normal;
    }

    p {
      font-size: 16px;
      margin: 0;
    }

    &:after,
    &:before {
      content: "";
      position: absolute;
      top: 0;
      height: 100%;
    }

    &:first-child {
      &:before {
        left: 0;
        transform: translateX(-100%);
        width: 100vw;
      }
    }

    &:last-child {
      &:after {
        right: 0;
        transform: translateX(100%);
        width: 100vw;
      }
    }

    &.block--1-2,
    &.block--1-3 {
      border-radius: 0 4px 4px 0;
    }

    &.block--2-2,
    &.block--3-3 {
      border-radius: 4px 0 0 4px;
    }

    &.block--2-3 {
      border-radius: 4px;
      max-width: 360px;
    }

    .btn {
      &:hover {
        background-color: $green;
      }
    }
  }

  .block--newsletter {
    padding-top: 47px;
    h3 {
      margin-bottom: 9px;
      color: $pink;
    }

    .success-response,
    .response {
      margin-bottom: 5px;
      font-size: 16px;
      font-family: "Avenir Book";
      color: #4d4d4d;

      a {
        color: #f3576b;
      }
    }

    .form {
      display: flex;
      max-width: 350px;
      box-shadow: 0 2px 3px #b0b5b6;
      border-radius: 4px;

      .email {
        height: 60px;
        flex: 1 1 100%;
        padding: 15px;
        border-radius: 4px 0 0 4px;
      }

      .btn {
        height: 60px;
        flex: 0 0 136px;
        border-radius: 0 4px 4px 0;
      }
    }
  }

  .block--link_box {
    &.block--3-3 {
      padding-left: 29px;
    }

    h3 {
      position: relative;
      display: inline-block;
      padding-right: 37px;

      &:after {
        content: "";
        position: absolute;
        right: 0;
        top: 50%;
        width: 15px;
        height: 26px;
        background-image: url(arrow-right-round.svg);
        background-size: contain;
        transform: translateY(-50%);
        transition: transform 0.3s;
      }
    }

    a:hover {
      h3:after {
        transform: translate(5px, -50%);
      }
    }
  }

  .divider {
    margin: 5px;
    border-right: 6px dotted $green;
  }

  @media #{$tablet-portrait} {
    .blocks {
      display: block;

      .block {
        padding: 53px 16px 24px;
        border-radius: 0;
      }
    }

    .page-width {
      padding: 0;
    }

    .block.block--2-3 {
      max-width: 100%;
    }

    .block--link_box.block--3-3 {
      padding-left: 16px;
    }

    .divider {
      margin: 5px 13px;
      border-right: 0;
      border-top: 6px dotted #a9dd61;
    }

    .block--newsletter {
      .form {
        max-width: 100%;
      }
    }
  }
}

.footer {
  .flex {
    display: flex;
  }

  .page-width {
    max-width: 1300px;
    padding: 0 30px;
  }

  @media #{$mobile} {
    .page-width {
      padding: 0 16px;
    }
  }
}

.footer__top {
  padding-bottom: 20px;
  h6 {
    margin-bottom: 15px;
    color: #4d4d4d;
    text-transform: uppercase;
    letter-spacing: 1px;
    font-family: "KG HAPPY Solid";
    font-size: 14px;
  }

  ul {
    list-style: none;
    padding: 0;
    margin: 0;
  }

  li {
    margin-bottom: 10px;

    &:last-child {
      margin-bottom: 0;
    }

    a {
      font-size: 14px;
      font-family: "Avenir Book";
      color: #4d4d4d;

      &:hover {
        color: $pink;
      }
    }

    &.active {
      a {
        color: $pink;
      }
    }
  }

  .box {
    flex: 0 1 20.7%;
    padding-right: 20px;
    &:last-child {
      padding-right: 0;
    }
  }

  .box:nth-child(5n) {
    margin-left: auto;
  }

  .box--social {
    flex: 0 0 216px;
    max-width: 216px;
    padding-top: 43px;

    ul {
      display: flex;
      justify-content: center;
    }

    .social-icons {
      a {
        display: block;
        width: 32px;
        height: 32px;
        background-size: contain;
        background-position: center center;
        background-repeat: no-repeat;
        transition: transform 0.3s;

        &:hover {
          transform: scale(1.1);
        }

        &.icon-facebook {
          background-image: url({{"facebook.png"|asset_url}});
        }
        &.icon-instagram {
          background-image: url({{"instagram.png"|asset_url}});
        }
        &.icon-pinterest {
          background-image: url({{"pinterest.png"|asset_url}});
        }
        &.icon-twitter {
          background-image: url({{"twitter.png"|asset_url}});
        }
      }
    }

    li {
      padding: 0 12px;
    }
  }

  @media screen and (max-width: 1024px) {
    .flex {
      flex-wrap: wrap;
    }

    .box--social,
    .box {
      flex: 1 1 33%;
      max-width: 33%;
    }
  }

  @media #{$mobile} {
    padding-top: 4px;
    padding-bottom: 12px;
    h6 {
      margin-top: 20px;
      margin-bottom: 19px;
    }

    .box {
      flex: 1 1 50%;
      max-width: 50%;
      margin-bottom: 10px;
      padding-right: 0;

      &:nth-child(2n) {
        padding-left: 20px;
      }
    }

    .box--menu {
      li {
        margin-bottom: 18px;
      }
    }

    .box--social {
      flex: 1 1 100%;
      max-width: 100%;
      border-top: 6px dotted #a9dd61;
      padding-top: 22px;
      margin-top: 12px;

      li {
        padding: 0 11px;

        a {
          width: 37px;
          height: 37px;
          font-size: 24px;
        }
      }
    }

    .foot-logo {
      display: none;
    }
  }
}

.foot-logo {
  display: block;
  font-size: 0;
  margin-bottom: 10px;

  img {
    width: 100%;
  }
}

.footer__bottom {
  padding: 21px 0;
  border-top: 6px dotted $green;

  .flex {
    justify-content: space-between;
  }

  li a,
  p {
    font-size: 14px;
  }

  p {
    color: rgba(77, 77, 77, 0.7);
  }

  li {
    display: inline-block;
    list-style: none;
  }

  li + li {
    position: relative;
    margin-left: 9px;
    padding-left: 5px;

    &:after {
      content: "|";
      position: absolute;
      left: -6px;
      top: 0;
    }
  }

  a {
    color: #4d4d4d;

    &:hover {
      color: $pink;
    }
  }

  ul,
  p {
    margin: 0;
  }

  @media #{$mobile} {
    .flex {
      display: block;
      text-align: center;

      p {
        margin-bottom: 7px;
      }

      ul {
        padding: 0;
      }
    }
  }
}

/* ------------------------------------------------------------ *\
  Intro
\* ------------------------------------------------------------ */

.intro {
  display: flex;
  flex-flow: row wrap;
  align-items: center;
  background-size: cover;
  background-position: center center;
  background-repeat: no-repeat;
  min-height: 550px;

  @media all and (-ms-high-contrast: none), (-ms-high-contrast: active) {
    height: 550px;
    min-height: 0;
  }

  .page-width {
    max-width: 1310px;
    width: 100%;
    padding: 0 30px;
  }

  .intro__inner {
    max-width: 595px;
    text-align: center;
  }

  .intro__title {
    margin: 0 0 10px;
    font-size: 40px;
    line-height: 1.325;
    font-family: "KG HAPPY Solid", sans-serif;
    color: $pink;
  }

  .intro__description {
    margin: 0 0 15px;
    font: 400 18px/1.444444 "Avenir Book", sans-serif;
    color: #4d4d4d;
  }

  .intro__btn {
    min-width: 165px;
    letter-spacing: 0.0625em;
  }

  @media #{$mobile} {
    background-image: none !important;
    min-height: 0;

    .intro__inner {
      position: relative;
      margin: 0 auto;
      padding: 0 16px 30px;
    }

    .intro__image {
      margin-bottom: 15px;
    }

    .intro__title {
      margin-bottom: 0;
      font-size: 26px;
      line-height: 1.6;
      padding: 0 30px;
    }

    .intro__description {
      padding: 0 40px;
    }

    .intro__btn {
      width: 100%;
      padding: 16px 20px;
    }
  }
}

