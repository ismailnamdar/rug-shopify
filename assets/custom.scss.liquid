/* ------------------------------------------------------------ *\
  Section Inside
\* ------------------------------------------------------------ */

.section-inside {
  &.section-inside--reverse {
    .section__inner {
      flex-flow: row-reverse wrap;
    }

    .section__content {
      padding-right: calc((100vw - 940px) / 2);
      padding-left: 120px;
    }
  }

  .section__inner {
    display: flex;
    flex-flow: row wrap;
  }

  .section__image {
    flex: 1 1;

    img {
      max-width: none;
      width: 100%;
      height: 100%;
      margin-bottom: 0;
      object-fit: cover;
    }
  }

  .section__content {
    align-self: center;
    padding: 100px 0;
    padding-left: calc((100vw - 940px) / 2);
    padding-right: 120px;

    & > * {
      max-width: 400px;

      @media #{$mobile} {
        max-width: none;
      }
    }
  }

  .section__title {
    margin: 0 0 32px;
    font-family: "KG HAPPY Solid", sans-serif;
    font-size: 24px;
    line-height: 1.333333;
    color: #01BCBE;
  }

  .section__entry {
    margin-bottom: 25px;
    font-weight: 900;
    font-size: 18px;
    line-height: 1.444444;

    ul {
      list-style: none;
    }

    li {
      margin-bottom: 9px;

      &:before {
        content: "\2022";
        color: #f3576b;
        font-weight: bold;
        display: inline-block;
        width: 1em;
        margin-left: -1em;
      }
    }
  }

  .section__btn {
    background-color: transparent;
    color: inherit;
    border: 1px solid currentColor;

    &:hover {
      background-color: $green;
      border-color: transparent;
    }
  }

  @media #{$tablet-portrait} {
    .section__content {
      flex: 0 0 50%;
      max-width: 50%;
      padding: 60px 30px;
    }

    &.section-inside--reverse {
      .section__content {
        padding: 60px 30px;
      }
    }
  }

  @media #{$mobile} {
    .section__inner {
      padding: 0 8px;
    }

    .section__image {
      border: 8px solid #fff;
      border-radius: 5px;
      flex: auto;

      img {
        -webkit-box-shadow: 0px 0px 5px 1px rgba(50, 50, 50, 0.49);
        -moz-box-shadow:    0px 0px 5px 1px rgba(50, 50, 50, 0.49);
        box-shadow:         0px 0px 5px 1px rgba(50, 50, 50, 0.49);
        border-radius: 5px;
      }
    }

    .section__content {
      max-width: 100%;
      padding: 0;
      flex: auto;
      padding: 27px 12px;
    }

    .btn {
      display:block;
      width: 100%;
    }

    .section__title {
      font-size: 20px;
      text-align: center;
      margin-bottom: 0;
    }

    .section__btn {
      width: 100%;
    }

    .section__entry ul {
      padding-left: 17px;
    }

    &.section-inside--reverse {
      .section__content {
        padding: 27px 12px;
        order: 2;
      }

      .section__image {
        order: 1;
      }
    }
  }
}

