/* ------------------------------------------------------------ *\
  How it Works
\* ------------------------------------------------------------ */

.steps-heading {
  margin-bottom: 50px;

  .heading {
    margin: 43px 0 8px;
    font-size: 40px;
    text-transform: none;
  }

  p {
    margin: 0;
    font-size: 18px;
    line-height: 1.444444;
  }

  @media #{$mobile} {
    margin-bottom: 30px;
    .heading {
      font-size: 32px;
    }
  }
}

.step-row {
  margin: 0 auto 66px;
  max-width: 1020px;

  .flex {
    justify-content: space-between;
    align-items: center;
  }

  .step__image {
    order: 1;
    flex: 0 1 45%;
    text-align: center;

    .image {
      position: relative;
      width: 275px;
      height: 275px;
      margin: 0 auto;
      border-radius: 100%;
      background-color: #e4f4f6;

      img {
        position: absolute;
        top: 50%;
        left: 50%;
        width: 100%;
        max-width: 80%;
        height: auto;
        transform: translate(-50%, -50%);
      }
    }
  }

  .step__content {
    flex: 0 1 45%;
    text-align: center;
    order: 3;
  }

  .step__num {
    align-self: flex-start;
    order: 2;
    padding-top: 64px;

    span {
      position: relative;
      display: flex;
      align-items: center;
      justify-content: center;
      width: 48px;
      height: 48px;
      padding-bottom: 10px;
      background-color: $pink;
      border-radius: 100%;
      font-size: 32px;
      font-family: "KG HAPPY Solid";
      color: #fff;
      opacity: 0.5;
      line-height: 1;
      transition: opacity 0.3s;

      &:after {
        content: "";
        position: absolute;
        top: 100%;
        left: 50%;
        height: 265px;
        transform: translate(-50%, 12px);
        border-right: 5px dotted #a9dd61;
      }

      &:before {
        content: "";
        position: absolute;
        top: 100%;
        left: 50%;
        width: 5px;
        height: 0;
        transform: translate(-50%, 12px);
        background-color: #a9dd61;
        transition: height 0.5s;
        border-radius: 10px;
      }
    }

    &.visible {
      span {
        opacity: 1;
      }
    }

    &.load {
      span:before {
        height: 265px;
      }
    }
  }

  &:first-child {
    .step__num {
      span {
        opacity: 1;
      }
    }
  }

  &:last-child {
    margin-bottom: 50px;
    .step__num {
      span {
        &:after,
        &:before {
          display: none;
        }
      }
    }
  }

  &:nth-child(2n) {
    .step__image {
      order: 3;
    }

    .step__content {
      order: 1;
    }
  }

  h3 {
    margin: 0 0 10px;
    font-size: 24px;
    font-family: "KG HAPPY Solid";
    color: #01bcbe;
  }

  p {
    margin: 0;
    font-size: 18px;
  }

  @media #{$mobile} {
    margin-bottom: 25px;

    .flex {
      display: block;
    }

    .step__num {
      padding: 0 0 10px;

      span {
        width: 48px;
        height: 48px;
        margin: -24px auto 0;

        &:before,
        &:after {
          display: none;
        }
      }
    }

    .step__image {
      .image {
        width: 275px;
        height: 275px;
      }
    }

    h3 {
      font-size: 20px;
    }

    &:last-child {
      margin-bottom: 37px;
    }
  }
}

